<Group view="code" labelreference="L68" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">preKbd := Matrix(4,4):</Text-field><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i from 1 to 4 do
    for j from 1 to 4 do
        preKbd[i,j] := nabla_shapes_eul[i,1..3].
                       Transpose(nabla_shapes_eul[j,1..3]):
        preKbd[i,j] := simplify(preKbd[i,j]):
        preKbd[i,j] := subs(x = lambda*x, y = lambda*y,
                            preKbd[i,j]):
        preKbd[i,j] := simplify(preKbd[i,j]):
        preKbd[i,j] := subs(x = G[1,1], y = G[2,1], z = G[3,1],
                            preKbd[i,j]):
        preKbd[i,j] := simplify(preKbd[i,j]):
        preKbd[i,j] := preKbd[i,j]*Jsigma:
        preKbd[i,j] := simplify(preKbd[i,j]):
   od:
od:</Text-field><Text-field style="Text" layout="Normal"></Text-field>
</Input>
</Group>
