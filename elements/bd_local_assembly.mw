<Group view="code" labelreference="L60" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">nabla_shapes_eul := Matrix(4,3):</Text-field><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Vars := &lt;x,y,z&gt;:</Text-field><Text-field prompt="&gt; " style="Maple Input" layout="Normal">n := convert(n,Matrix):</Text-field><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i from 1 to 4 do
    for j from 1 to 3 do
        nabla_shapes_eul[i,j] := diff(shapes_eul[i,1],Vars[j]):
    od:
od:</Text-field><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i from 1 to 4 do
    projn := nabla_shapes_eul[i,1..3].n:
    projn := projn[1]:
    nabla_shapes_eul[i,1..3] := simplify(
                                         nabla_shapes_eul[i,1..3] - 
                                         projn*Transpose(n[1..3,1])
                                         ):
od:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L64" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
